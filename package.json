{
  "name": "ts-ethereum",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build --parallel --concurrency=100%",
    "build:packages": "turbo run build  --filter='./packages/*' --parallel --concurrency=100%",
    "check": "turbo run check --cache=local:r,remote:r --parallel --concurrency=100%",
    "clean": "turbo run clean --parallel --concurrency=100% && rm -rf node_modules",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "format": "biome format . --write",
    "test": "turbo run test --concurrency=100%",
    "prepare": "lefthook install",
    "sanity": "bun scripts/sanity-check.ts",
    "sanity:generate": "bun scripts/generate-snapshot.ts",
    "test:unit": "vitest run --project unit --project unit"
  },
  "devDependencies": {
    "@paulmillr/trusted-setups": "0.2.0",
    "@types/bun": "latest",
    "lefthook": "^1.11.1",
    "micro-eth-signer": "0.15.0",
    "tsx": "^4.20.6",
    "turbo": "^2.7.1",
    "@vitest/coverage-v8": "^4.0.16",
    "vitest": "^4.0.13",
    "vitest-when": "^0.10.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@biomejs/biome": "^2.3.4",
    "@hono/node-server": "^1.19.6",
    "@hono/standard-validator": "^0.2.0",
    "@js-sdsl/ordered-map": "^4.4.2",
    "@libp2p/interface": "^3.1.0",
    "@libp2p/interface-connection": "^5.1.1",
    "@libp2p/interfaces": "^3.3.2",
    "@libp2p/logger": "^6.2.2",
    "@multiformats/multiaddr": "^13.0.1",
    "@multiformats/multiaddr-matcher": "^3.0.1",
    "@noble/curves": "2.0.1",
    "@noble/hashes": "^2.0.1",
    "@peculiar/webcrypto": "^1.5.0",
    "@peculiar/x509": "^1.14.0",
    "@polkadot/wasm-crypto": "^7.5.3",
    "@scalar/hono-api-reference": "^0.9.28",
    "@types/debug": "^4.1.12",
    "any-signal": "^4.1.1",
    "benchmark": "^2.1.4",
    "bigint-crypto-utils": "^3.3.0",
    "body-parser": "^2.2.1",
    "chalk": "5.6.2",
    "connect": "^3.7.0",
    "cors": "^2.8.5",
    "elysia": "^1.4.16",
    "ethereum-cryptography": "^3.2.0",
    "hono": "^4.10.8",
    "hono-openapi": "^1.1.2",
    "it-pushable": "^3.2.3",
    "it-stream-types": "^2.0.2",
    "jayson": "^4.2.0",
    "level": "^10.0.0",
    "lru-cache": "^11.2.2",
    "main-event": "^1.0.1",
    "mcl-wasm": "^2.0.0",
    "memory-level": "^3.1.0",
    "multiformats": "^13.4.1",
    "node-dir": "^0.1.17",
    "p-event": "^7.0.1",
    "prom-client": "^15.1.3",
    "protons-runtime": "^5.6.0",
    "race-signal": "^2.0.0",
